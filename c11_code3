#include <stdio.h>
#include <string.h>

int main() {
	
	struct personaldata {
    	char first_name[20];
    	char last_name[20];
	};
	
	struct personaldata_temp {
    	char first_name_temp[20];
    	char last_name_temp[20];
	};
	
    int i = 0, num;
    struct personaldata person[50];
	struct personaldata_temp person_temp[50];
    
	
    printf("Enter first name:");
    scanf("%s", person_temp[0].first_name_temp);
	
    printf("Enter last name:");
    scanf("%s", person_temp[0].last_name_temp);

    FILE *fp1 = fopen("phonedir.txt", "r+");
    
    if(fp1 == NULL){
        printf("Error opening file!");
        return 1;
    } 
	else {
		fscanf(fp1, "%d", &num);
		for(i=0; i < num; i++){
			fscanf(fp1, "%s %s", person[i].first_name, person[i].last_name);
			if(strcmp(person[i].first_name, person_temp[i].first_name_temp) == 0 && strcmp(person[i].last_name, person_temp[i].last_name_temp) == 0){
            }
		}
		printf("Data removed from the directory.");
		fclose(fp1);
	}
	
    return 0;
}
