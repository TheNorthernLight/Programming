#include<stdio.h> 

int main()
{
	int matrix[5][5] =
	{
	4, 6, 25, 88, 5,
	34, 5, 300, 4, 65,
	78, 43, 11, 90, 125,
	98, 585, 12, 63, 21,
	45, 35, 9, 5, 1
	};
	
	int x, y, sum=0;
	printf("In the array:\n");
	
	for(x=0; x < 5; x++){
		
		
		for(y=0; y < 5; y++){
			
			sum = sum + matrix[x][y];
			printf("%d ", matrix[x][y]);
		}
		printf("\n");
	}
	
	
	printf("\n\nthe sum of the elements is %d ", sum);
	
  return 0;
}
