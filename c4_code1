#include <stdio.h>
int main()
{  
int number1, number2, sum, div, multi;

printf("Enter the first number:");
scanf("%d", &number1);

printf("Enter the second number:");
scanf("%d", &number2);

sum = number1 + number2;
div = number1 - number2;
multi = number1 * number2;
	
	
printf("%d + %d = %d\n", number1, number2, sum);
printf("%d - %d = %d\n", number1, number2, div);
printf("%d * %d = %d", number1, number2, multi);
	
	
return 0;
}
