#include <iostream>
#include <string>
using namespace std;

// your code


class Dog 
{
public:
    int age;
    string name, race, voice;

    Dog(int x_age, string x_name, string x_race, string x_voice) 
    {
        age = x_age;
        name = x_name;
        race = x_race;
        voice = x_voice;
    };

    void PrintInformation();
    string Bark();
};

void Dog::PrintInformation()
{
    cout << "Name: " << name << endl;
    cout << "Age: " << age << endl;
    cout << "Race: " << race << endl;
}

string Dog::Bark()
{
    return voice;
}



int main()
{
  Dog buffy(2, "Buffy", "Bulldog", "Hau!!!");
  buffy.PrintInformation();
  cout << "Dog says: " << buffy.Bark();
}  
